	author = {Mani, Anandi and Mullainathan, Sendhil and Shafir, Eldar and Zhao, Jiaying},
	month = aug,
	year = {2013},
	pmid = {23990553},
	pages = {976--980},
	file = {Snapshot:/Users/ana812/Zotero/storage/V9G95ITD/976.html:text/html}
}

