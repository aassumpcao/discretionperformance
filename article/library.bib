}

@article{voigt_economic_2015,
	title = {Economic {Growth} and {Judicial} {Independence}, a {Dozen} {Years} {On}: {Cross}-country {Evidence} {Using} an {Ppdated} {Set} of {Indicators}},
	volume = {38},
	issn = {0176-2680},
	url = {http://www.sciencedirect.com/science/article/pii/S0176268015000051},
	doi = {10.1016/j.ejpoleco.2015.01.004},
	abstract = {Over 10years ago, Feld and Voigt (2003) introduced an indicator for objectively measuring the actual independence of the judiciary and demonstrated its utility in a large cross-section of countries. The indicator has been widely used, but also criticized. Many new indicators for judicial independence have been developed since. Yet, all of them are based on subjective evaluations by experts or confined to measuring the legally prescribed level of independence. This paper presents more recent objective data on de jure and de facto judicial independence (JI) and strongly confirms previous results that de jure JI is not systematically related to economic growth, whereas de facto JI is highly significantly and robustly correlated with growth. In addition, we show that the effect of de facto JI depends on the institutional environment, but not on a country’s initial per capita income.},
	journal = {European Journal of Political Economy},
	author = {Voigt, Stefan and Gutmann, Jerg and Feld, Lars P.},
	month = jun,
	year = {2015},
	keywords = {Economic growth, Constitutional economics, Governance, Judicial independence, Measuring institutions, Rule of law},
	pages = {197--211}
}

@article{helmke_logic_2002,
	title = {The {Logic} of {Strategic} {Defection}: {Court}-{Executive} {Relations} in {Argentina} under {Dictatorship} and {Democracy}},
	volume = {96},
	issn = {00030554, 15375943},
	url = {www.jstor.org/stable/3118026},
	abstract = {[Building on the separation-of-powers approach in American politics, this article develops a new micro-level account of judicial decision-making in contexts where judges face institutional insecurity. Against conventional wisdom, I argue that under certain conditions the lack of judicial independence motivates judges to "strategically defect" against the government once it begins losing power. The result is a reverse legal-political cycle in which antigovernment decisions cluster at the end of weak governments. Original data on more than 7,500 individual decisions by Argentine Supreme Court justices (1976-1995) are used to test hypotheses about why, when, and in which types of cases judges are likely to engage in strategic defection. Consistent with the theory's predictions, the results of the analysis show a significant increase in antigovernment decisions occurring at the end of weak dictatorships and weak democratic governments. Examining subsets of decisions and controlling for several additional variables further corroborate the strategic account.]},
	number = {2},
	journal = {American Political Science Review},
	author = {Helmke, Gretchen},
