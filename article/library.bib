	doi = {10.1016/B978-0-444-59517-1.00010-6},
	keywords = {Endogenous technical change, Firms, Immigrant enclaves, Immigration, Innovation, International: F16, F22, Labor markets, Labor supply, Labor: J2, J3, J61, Production technique and innovation: O31, O33, Productivity, Regional: R11, R12, Skill complementarities},
